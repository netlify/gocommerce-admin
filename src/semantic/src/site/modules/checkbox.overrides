/*******************************
         Site Overrides
*******************************/
.ui.checkbox .box:before, .ui.checkbox label:before {
    box-shadow: inset 0px 0.5px 0.5px 0px rgba(0, 0, 0, 0.38);
}
.ui.checkbox input:checked~.box:after, .ui.checkbox input:checked~label:after {
    opacity: 1;
    color: #ffffff;
    background: #3b99fc;
    border-radius: 3px;
}
